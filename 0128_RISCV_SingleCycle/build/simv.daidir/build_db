#!/bin/sh -e
# This file is automatically generated by VCS.  Any changes you make
# to it will be overwritten the next time VCS is run.
vcs '-full64' '-sverilog' '+incdir+./rtl' '-debug_access+all' '-kdb' '-lca' '-race=all' '-timescale=1ns/100ps' '-o' './build/simv' '-l' './build/comp.log' '-f' 'files.f'  -static_dbgen_only -daidir=$1 2>&1
